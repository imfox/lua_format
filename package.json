{
  "name": "lua_styles",
  "version": "1.1.1",
  "main": "./dist/cjs/index.cjs",
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc -p tsconfig.esm.json && mv ./dist/esm/index.js ./dist/esm/index.mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json && mv ./dist/cjs/index.js ./dist/cjs/index.cjs",
    "test": "node test/index.js"
  },
  "author": "fox",
  "license": "MIT",
  "description": "Only beautify your code (indentation, spacing, alignment), your code lines will not be increased or decreased.",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imfox/lua_format.git"
  },
  "keywords": [
    "lua",
    "format"
  ],
  "bugs": {
    "url": "https://github.com/imfox/lua_format/issues"
  },
  "homepage": "https://github.com/imfox/lua_format#readme"
}